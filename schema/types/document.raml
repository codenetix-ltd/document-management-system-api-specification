#%RAML 1.0 DataType

type: object
properties:
  id: integer
  name: string
  ownerId: integer
  substituteDocumentId: integer
  attributeValues:
    type: array
    items:
      type: object
      properties:
        id: integer
        value: string
  templateId: integer
  createdAt: integer
  updatedAt: integer
  labelId: integer
  label: !include label.raml
  owner: !include user.raml
  template: !include template.raml
  version: integer

example: !include ../examples/Document.json
